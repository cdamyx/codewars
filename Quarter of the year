const quarterOf = (month) => {
    switch (true) {
      case month > 9:
        return 4;
        break;
      case month > 6:
        return 3;
        break;
      case month > 3:
        return 2;
        break;
      default:
        return 1;
        break;
    }
  
}
//time: O(1), space: O(1)
